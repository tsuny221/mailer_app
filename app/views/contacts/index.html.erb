<table class='table'>
  <thead>
    <tr>
      <th>件名</th>
      <th>本文</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.subject %></td>
        <td><%= contact.message %></td>
      </tr>
    <% end %>
  </tbody>
</table>
